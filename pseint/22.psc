Proceso ejercicio_22
	//. Pedir dos números y mostrarlos ordenados de mayor a menor.
	
	//definir 
	Definir num1,num2 Como Entero;
	
	//entrada
	Escribir "escribir dos numeros";
	leer num1,num2;
	
	//Proceso 
	si (num1>num2) Entonces
			Escribir " ",num1," ",num2;
		SiNo
			Escribir " ",num2," ",num1;
	FinSi
FinProceso
