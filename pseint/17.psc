Proceso Ejercicio_17
	//pedir dos numero y decir si son iguales o no
	
	//definicion
	Definir num1,num2 Como Entero;
	//entrada
	Escribir "digite el numero 1 y el numero 2";
	leer num1, num2;
	//Proceso - salida
	si (num1 = num2) Entonces
		Escribir "los numeros son iguales";
